{
    "compileOnSave": false,
    "filesGlob": [
        "../typings/**/*.*.ts",
        "!../typings/jasmine/jasmine.d.ts",
        "**/*.{ts,tsx}"
    ],
    "compilerOptions": {
        "jsx": "preserve",
